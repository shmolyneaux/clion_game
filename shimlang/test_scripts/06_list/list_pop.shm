# Test list pop method
let lst = [1, 2, 3, 4, 5];

let val1 = lst.pop();
assert(val1 == 5);
assert(lst.len() == 4);

let val2 = lst.pop();
assert(val2 == 4);
assert(lst.len() == 3);

# Pop from specific index
let val3 = lst.pop(0);
assert(val3 == 1);
assert(lst.len() == 2);
assert(lst[0] == 2);
assert(lst[1] == 3);

# Pop with negative index
let val4 = lst.pop(-1);
assert(val4 == 3);
assert(lst.len() == 1);

# Pop last remaining element
lst.pop();
assert(lst.len() == 0);

# Pop from empty list returns None (no default)
let val_none = lst.pop();
assert(val_none == None);

# Pop from empty list with default
let val_default = lst.pop(-1, -1);
assert(val_default == -1);

print("List pop passed");
