// Test sorting with a key function
let lst = [5, 2, 8, 1, 9, 3];
// Sort by negative values (reverse order)
let sorted_lst = lst.sort(fn(x) { 0 - x });

assert(sorted_lst[0] == 9);
assert(sorted_lst[1] == 8);
assert(sorted_lst[2] == 5);
assert(sorted_lst[3] == 3);
assert(sorted_lst[4] == 2);
assert(sorted_lst[5] == 1);

print("Sort with key passed");
