struct Point {
    x,
    y
}

let p = Point(2.0, 3.0);
print(p);

struct Person {
    name,
    age,
    city
}

let person = Person("Alice", 30, "NYC");
print(person);

// Test with integer values
struct Vec2 {
    x,
    y
}

let v = Vec2(5, 10);
print(v);

// Empty struct
struct Empty {}
let e = Empty();
print(e);

// Test with nested struct
struct Rectangle {
    top_left,
    bottom_right
}

let rect = Rectangle(Point(1.0, 2.0), Point(3.0, 4.0));
print(rect);

// Test with bool and list
struct Config {
    enabled,
    values
}

let cfg = Config(true, [1, 2, 3]);
print(cfg);
